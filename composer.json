{
    "name": "face/face",
    "type": "library",
    "version": "0.1.1-BETA1",
    "description": "Modern & Friendly ORM for php5.4",
    "keywords": ["ORM"],
    "homepage": "https://github.com/face-orm/face",
    "license": "MIT",
    "authors": [
        {
            "name": "Soufiane Ghzal",
            "email": "sghzal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-0" : {
            "Face" : "lib/"
        }
    },
    "require-dev": {
        "php": ">=5.4",
        "phpunit/PHPUnit": "4.4.5",
        "phpunit/dbunit" : "1.2.*@dev",
        "codeclimate/php-test-reporter": "dev-master"
    },

    "autoload-dev": {
        "psr-0" : {

            "Test" : "./tests"
        },
        "classmap" : ["./tests/models"]
    }
}
